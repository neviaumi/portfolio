{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "next"],
  "rules": {
    "padding-line-between-statements": [
      "error",
      { "blankLine": "always", "next": "*", "prev": "import" },
      { "blankLine": "any", "next": "import", "prev": "import" }
    ]
  }
}
